    "@types/resolve": "1.20.2",
    "deepmerge": "^4.2.2",
    "is-builtin-module": "^3.2.0",
    "is-module": "^1.0.0",
    "resolve": "^1.22.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
