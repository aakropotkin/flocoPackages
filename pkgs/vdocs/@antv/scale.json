    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "limit-size": "^0.1.4",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^26.5.1",
    "tslib": "^2.2.0",
    "typescript": "^4.1.5"
