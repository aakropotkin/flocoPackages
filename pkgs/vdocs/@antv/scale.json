  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.ts$",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "limit-size": [
    {
      "path": "dist/scale.min.js",
      "limit": "12 Kb",
      "gzip": true
    },
    {
      "path": "dist/scale.min.js",
