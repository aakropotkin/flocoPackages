      "pre-commit": "run-s lint build test"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts}": [
      "tslint -c tslint.json --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "runner": "jest-electron/runner",
    "testEnvironment": "jest-electron/environment",
    "preset": "ts-jest",
    "collectCoverage": false,
    "collectCoverageFrom": [
