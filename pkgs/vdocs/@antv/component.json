    "@antv/g-base": "^0.5.9",
    "@antv/matrix-util": "^3.1.0-beta.1",
    "@antv/path-util": "~2.0.7",
    "@antv/scale": "~0.3.1",
    "@antv/util": "~2.0.0",
    "fecha": "~4.2.0",
    "tslib": "^2.0.3"
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts}": [
      "tslint -c tslint.json --fix",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "runner": "jest-electron/runner",
    "testEnvironment": "jest-electron/environment",
    "preset": "ts-jest",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "testRegex": "/tests/.*spec\\.ts?$"
  },
