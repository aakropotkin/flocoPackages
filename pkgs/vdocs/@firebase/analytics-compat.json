  "key": "@firebase/analytics-compat/0.1.17",
  "hasBin": false,
  "isSimple": true,
  "flocoPkgType": "simple",
  "scopeDir": "@firebase/",
  "scope": "firebase",
  "bname": "analytics-compat",
