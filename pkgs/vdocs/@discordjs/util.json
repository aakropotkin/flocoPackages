    "@types/node": "^16.11.56",
    "@vitest/coverage-c8": "^0.22.1",
    "cross-env": "^7.0.3",
    "downlevel-dts": "^0.10.1",
    "eslint": "^8.23.0",
    "eslint-config-neon": "^0.1.31",
    "prettier": "^2.7.1",
    "tsd": "^0.23.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "vitest": "^0.22.1"
  },
  "engines": {
    "node": ">=16.9.0"
  },
