    "@vitest/coverage-c8": "^0.24.0",
    "cross-env": "^7.0.3",
    "downlevel-dts": "^0.10.1",
    "esbuild-plugin-version-injector": "^1.0.0",
    "eslint": "^8.24.0",
    "eslint-config-neon": "^0.1.35",
    "prettier": "^2.7.1",
    "tsup": "^6.2.3",
