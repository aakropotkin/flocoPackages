    "mocha": "9.2.x",
    "nyc": "^15.1.0",
    "objstorage": "^1.0.0",
    "pre-commit": "1.2.x",
    "require-poisoning": "^2.0.0",
    "webpack": "4.x",
    "webpack-bundle-size-analyzer": "^3.0.0",
    "webpack-cli": "3.x"
  },
  "dependencies": {
    "colorspace": "1.1.x",
    "enabled": "2.0.x",
    "kuler": "^2.0.0"
  },
