{
  "name": "npm-packlist",
  "version": "7.0.2",
  "description": "Get a list of the files to add from a folder into an npm package",
  "directories": {
    "test": "test"
  },
  "main": "lib/index.js",
  "dependencies": {
    "ignore-walk": "^6.0.0"
  },
