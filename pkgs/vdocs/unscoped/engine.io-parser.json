    "test:node": "nyc mocha -r ts-node/register test/index.ts",
    "test:browser": "zuul test/index.ts --no-coverage",
    "format:check": "prettier --check 'lib/**/*.ts' 'test/**/*.ts'",
    "format:fix": "prettier --write 'lib/**/*.ts' 'test/**/*.ts'"
  },
  "repository": {
