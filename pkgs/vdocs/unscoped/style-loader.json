    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "file-loader": "^6.2.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jsdom": "^18.0.0",
    "lint-staged": "^11.0.1",
    "memfs": "^3.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "semver": "^7.3.4",
