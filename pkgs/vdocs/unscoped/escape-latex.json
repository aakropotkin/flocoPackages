    "integrity": "sha512-nV5aVWW1K0wEiUIEdZ4erkGGH8mDxGyxSeqPzRNtWP7ataw+/olFObw7hujFWlVjNsaDFw5VZ5NzVSIqRgfTiw==",
    "tarball": "https://registry.npmjs.org/escape-latex/-/escape-latex-1.2.0.tgz"
  },
