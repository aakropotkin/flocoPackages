    "integrity": "sha512-3aYBPeUSBUCs/vCOYolbyzhsQ6IDm1DeJgfhHVbW+6kq8YhWjkk2SUhYtBxd3lZPNsqmJGzYH9shKINhSVbEzw==",
    "tarball": "https://registry.npmjs.org/yjs/-/yjs-13.5.42.tgz"
  },
  "directories": {},
