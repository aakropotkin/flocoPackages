    "lint": "eslint --cache src/**/*.mjs test/**/*.mjs test-lib/**/*.mjs",
    "format": "npm run lint -- --fix",
    "coverage": "nyc _mocha -- test --recursive; echo \"\nCoverage report is available at ./coverage/lcov-report/index.html\"",
