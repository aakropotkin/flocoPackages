    "integrity": "sha512-EmTr31wppcaIAgblChZiuN/l9Y7DPyw8Xtbg7fIVngn6zMW+IEBJDJngeKC3x6wr0V/vcA2wqeFnaw1bFJbDdA==",
    "tarball": "https://registry.npmjs.org/es6-shim/-/es6-shim-0.35.6.tgz"
  },
