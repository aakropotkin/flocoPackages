    "jsdom": "^16.2.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "^10.5.3",
    "mocha": "^7.0.1",
    "mochify": "^6.6.0",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "proxyquire": "^2.1.3",
    "proxyquire-universal": "^2.1.0",
    "proxyquireify": "^3.2.1",
    "sinon": ">=9"
  },
  "dependencies": {
    "@sinonjs/commons": "^1.8.3",
    "@sinonjs/fake-timers": ">=5",
    "@sinonjs/text-encoding": "^0.7.1",
    "just-extend": "^4.0.2",
    "path-to-regexp": "^1.7.0"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --check",
    "*.js": "eslint --quiet"
  },
  "husky": {
    "hooks": {
