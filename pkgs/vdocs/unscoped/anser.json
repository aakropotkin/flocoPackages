{
  "name": "anser",
  "version": "2.1.1",
  "description": "A low level parser for ANSI sequences.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/IonicaBizau/anser.git"
  },
