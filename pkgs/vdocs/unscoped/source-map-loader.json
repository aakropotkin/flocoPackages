{
  "name": "source-map-loader",
  "version": "4.0.1",
  "description": "extracts inlined source map and offers it to webpack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webpack-contrib/source-map-loader.git"
