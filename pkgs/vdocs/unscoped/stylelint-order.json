  "scripts": {
    "pretest": "eslint . --max-warnings=0 && prettier '**/*.js' --check",
    "test": "jest",
    "jest": "jest",
    "watch": "jest --watch",
    "coverage": "jest --coverage",
    "fix": "eslint . --fix --max-warnings=0 && prettier '**/*.js' --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  },
