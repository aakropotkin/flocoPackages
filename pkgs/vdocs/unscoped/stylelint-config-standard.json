    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "np": "^7.6.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "remark-cli": "^11.0.0",
    "stylelint": "^14.14.0"
