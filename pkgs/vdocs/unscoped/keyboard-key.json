        "lines": 50,
        "statements": 50
      }
    },
    "roots": [
      "./src",
      "./test"
    ],
    "testMatch": [
      "**/*.spec.js"
    ],
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js?(on|x)": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
