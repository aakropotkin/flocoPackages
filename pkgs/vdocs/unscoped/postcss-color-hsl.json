    "pretest": "npm run lint",
    "test:ava": "ava",
    "test": "nyc npm run test:ava",
    "prebuild": "npm-run-all clean lint",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
