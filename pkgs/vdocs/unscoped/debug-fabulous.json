    "config": "^3.2.2",
    "coveralls": "^3.0.4",
    "del": "^4.1.1",
    "esm": "^3.2.22",
    "gulp-run": "^1.7.1",
    "gulp-typescript": "^5.0.1",
    "hook-std": "0.X",
    "prettier": "^1.18.2",
    "react": "^16.8",
    "rollup": "^1.20.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-typescript": "^1.0.1",
    "sort-package-json": "^1.21.0",
