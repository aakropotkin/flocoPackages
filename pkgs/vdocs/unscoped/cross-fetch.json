    "codecov": "3.8.3",
    "cz-conventional-changelog": "3.3.0",
    "express": "4.17.1",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "mocha": "8.3.2",
    "mocha-headless-chrome": "3.1.0",
    "nock": "13.1.3",
    "nyc": "15.1.0",
    "open-cli": "6.0.1",
    "rollup": "2.58.0",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-terser": "7.0.2",
    "semver": "7.3.5",
    "serve-index": "1.9.1",
    "standard": "16.0.4",
    "standard-version": "9.3.1",
    "typescript": "4.4.4",
    "webpack": "5.58.2",
