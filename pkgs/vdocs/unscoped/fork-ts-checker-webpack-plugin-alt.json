    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "concurrent": false,
    "linters": {
      "*.js": [
        "eslint --fix"
      ],
      "*.ts": [
        "tslint --fix"
      ],
      "*.{js,ts}": [
        "prettier --write",
        "git add"
      ]
    }
  },
