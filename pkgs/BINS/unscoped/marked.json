    "tarball": "https://registry.npmjs.org/marked/-/marked-4.1.1.tgz"
  },
  "directories": {},
  "hasInstallScript": false,
  "ident": "marked",
  "key": "marked/4.1.1",
  "hasBin": true,
  "isSimple": false,
